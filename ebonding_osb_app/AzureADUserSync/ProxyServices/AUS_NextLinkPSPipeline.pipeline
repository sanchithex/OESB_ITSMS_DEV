            <con:stage id="_StageId-a013dc6.3d18e46e.0.1661baa6a7b.N7bdd" name="CheckCurrentBatchStatus" xmlns:con1="http://www.bea.com/wli/sb/stages/logging/config" xmlns:con3="http://www.bea.com/wli/sb/stages/transform/config">
                <con:context/>
                <con:actions>
                    <con3:assign varName="BatchId">
                        <con2:id>_ActionId-a013dc6.3d18e46e.0.1661baa6a7b.N7ba5</con2:id>
                        <con3:expr>
